print(model)



import os

for filename in os.listdir():
    if filename.endswith('.pkl'):
        new_name = filename.lower().replace(' ', '_')
        if filename != new_name:
            os.rename(filename, new_name)
            print(f"Renamed: {filename} → {new_name}")




